# Provider Credentials
CLAUDE_CODE_ACCESS_TOKEN=sk-ant-xxxxx
OPENCODE_API_KEY=sk-xxxx-your-opencode-api-key
CLAUDE_CODE_API_KEY=sk-ant-yyyy

# Kimi OAuth (uses subscription credits - PRIMARY)
# Auto-refreshing OAuth tokens stored in ~/.opencode-kimi-auth/oauth.json

# Kimi API Key (uses API credits - FALLBACK)
# Get from https://platform.moonshot.cn/console/api-keys
KIMI_API_KEY=kimi-api-xxxx

# Linear Integration
LINEAR_CLIENT_ID=your-linear-client-id
LINEAR_CLIENT_SECRET=your-linear-client-secret
LINEAR_WEBHOOK_SECRET=<from Linear app settings>
LINEAR_API_TOKEN=<get from ~/.jinyang/config.json or create new OAuth token>

# Server Config
JINYANG_PORT=3001
JINYANG_HOST=0.0.0.0

# Health Check Config
PROVIDER_HEALTHCHECK_INTERVAL_SEC=30
PROVIDER_UNHEALTHY_THRESHOLD=3
